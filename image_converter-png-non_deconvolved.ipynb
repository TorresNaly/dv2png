   "source": [
    "# NAS credentials and connection information\n",
    "nas_host = '' #nas_server_hostname_or_ip'\n",
    "nas_user = '' #your_username\n",
    "nas_password = '' #your_password\n",
    "nas_share = ''" #your_parent_directory
   ]
  },
  {
   "cell_type": "code",
